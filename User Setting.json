{
    "editor.fontSize": 16,
    "editor.fontFamily": "Fira Code,Monaco,Menlo,Consolas,'Droid Sans Mono','Inconsolata','Courier New'",
    "editor.fontWeight": "400",
    "editor.tabSize": 4,
    "editor.cursorStyle": "line",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.cursorWidth": 3,
    "editor.dragAndDrop": false,
    "editor.links": false,
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#923e07",
        "editor.selectionHighlightBackground": "#426b3f",
        "minimapSlider.hoverBackground": "#e6111128"
    },
    "explorer.confirmDelete": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline" : true,
    "editor.minimap.showSlider": "always",
    "editor.renderIndentGuides": false,
    "explorer.confirmDragAndDrop": false,
    "window.zoomLevel": 0,
    "files.trimFinalNewlines": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "files.autoSave": "afterDelay",
    "workbench.sideBar.location": "left",
    "[html]": {},
    "editor.fontLigatures": true,
    "workbench.iconTheme": "material-icon-theme",
    "window.titleBarStyle": "native",
    "workbench.settings.editor": "json",
    "editor.suggestSelection": "recentlyUsed",
    "json.maxItemsComputed": 25000,
    "editor.maxTokenizationLineLength": 50000,
    "explorer.sortOrder": "type",
    "extensions.ignoreRecommendations": false,
    "[json]": {
        "editor.formatOnSave": true
    },
    "yaml.completion": true,
    "yaml.format.enable":true,
    "editor.rulers": [
        0,
        4,
        8
    ],
    "workbench.colorTheme": "Dracula",
    "redhat.telemetry.enabled": true,
    "yaml.maxItemsComputed": 10000,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.enabled": false
}
